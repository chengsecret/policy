<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.koisecret.auth.mapper.UserMapper">
    <insert id="addUser" parameterType="site.koisecret.auth.entity.User">
        insert into policy.user (name,phone,password,province,city,industry,profession,age)
        values(#{name}, #{phone}, #{password}, #{province}, #{city}, #{industry}, #{profession}, #{age} )
    </insert>

</mapper>
